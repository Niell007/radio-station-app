{
  "name": "radio-station-app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "init-db": "node scripts/init-db.js",
    "import-karaoke": "node scripts/import-karaoke.js",
    "dev:local": "wrangler dev --local",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.astro",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "migrate-to-prisma": "ts-node scripts/migrate-to-prisma.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "deploy": "wrangler deploy",
    "seed-db": "node scripts/seed-db.js",
    "analyze": "astro build --analyze",
    "clean": "rimraf dist .astro .turbo node_modules",
    "import-karaoke-sql": "node scripts/import-karaoke-sql.js",
    "deploy:pages": "npm run build && wrangler pages deploy dist",
    "deploy:preview": "npm run build && wrangler pages deploy dist --branch=$CF_PAGES_BRANCH",
    "deploy:production": "npm run build && wrangler pages deploy dist --branch=main --production",
    "check": "npm run type-check && npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.2.0",
    "@astrojs/react": "^3.6.3",
    "@astrojs/tailwind": "^5.1.2",
    "@cloudflare/ai": "^1.2.2",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@heroicons/react": "^2.1.1",
    "@prisma/client": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^4.16.16",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.6.0",
    "date-fns": "^4.1.0",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.7.2",
    "jose": "^5.9.6",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.15",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/better-sqlite3": "^7.6.12",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitest/coverage-v8": "^2.1.6",
    "eslint": "^9.16.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "prettier-plugin-astro": "^0.14.1",
    "prisma": "^6.0.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6",
    "wrangler": "^3.91.0"
  }
}
